<!-- DETAIL -->
<section id="two" class="wrapper">
	<div class="inner alt"> 
		<div class="content">
			<section>
				<p>DO results page looking for sammy


				results:search term: {{ searchTerm }} <br>
				results:search site: {{ searchSite }} <br>
				results:total results: {{ totalResults }} <br>
				results:returned results: {{ returnedResults }} <br>

				<table class="results">
					<!-- CONFIRMED PLAYERS -->
					<tbody>
						<tr style="background-color:#0078ff" ><td colspan = "3"><h3 style="color:#ffffff">Results</h3></td></tr>
					</tbody>
					<tbody>
						{{#each resultsArray}} 
		                    <tr>
								<td><img src={{userAvatarLink}} alt="" class="avatar_img" align="left"/></td>
								<td><a href={{userLink}}>{{userLogin}}</a></td>
								<td><a href={{pageLink}}>{{title}}</a> </td>
								
							</tr>
		                {{/each}}
					</tbody>
				</table>



				 





			</section>
		</div>					
	</div>		
</section>
		